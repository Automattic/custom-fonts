{
  "name": "jetpack-custom-fonts",
  "version": "0.0.1",
  "description": "Custom fonts plugin for WordPress",
  "repository": {
    "type": "git",
    "url": "git://github.com/Automattic/custom-fonts"
  },
  "author": "Automattic",
  "license": "GPL-2.0",
  "scripts": {
    "build": "npm run autoprefixer && npm run build:dev",
    "dist": "npm run autoprefixer && npm run build:dist",
    "autoprefixer:dev": "autoprefixer -m css/fonts-customizer.css",
    "autoprefixer:dist": "autoprefixer --no-map css/fonts-customizer.css",
    "build:dev": "npm run build:dev:admin && npm run build:dev:preview",
    "build:dev:admin": "browserify js/index.js --outfile js/jetpack-fonts.js --debug",
    "build:dev:preview": "browserify js/helpers/live-update.js --outfile js/jetpack-fonts-preview.js --debug",
    "build:dist": "npm run build:dist:admin && npm run build:dist:preview && npm run uglify",
    "build:dist:admin": "browserify js/index.js --outfile js/jetpack-fonts.js",
    "build:dist:preview": "browserify js/helpers/live-update.js --outfile js/jetpack-fonts-preview.js",
    "uglify": "npm run uglify:admin && npm run uglify:preview",
    "uglify:admin": "uglifyjs js/jetpack-fonts.js -c -m --output js/jetpack-fonts.js",
    "uglify:preview": "uglifyjs js/jetpack-fonts-preview.js -c -m --output js/jetpack-fonts-preview.js"
  },
  "bugs": {
    "url": "https://github.com/Automattic/custom-fonts/issues"
  },
  "homepage": "https://github.com/Automattic/custom-fonts",
  "devDependencies": {
    "babel-eslint": "^3.1.17",
    "backbone": "~1.1.2",
    "chai": "^2.2.0",
    "eslint": "^0.23.0",
    "grunt": "~0.4.5",
    "grunt-autoprefixer": "~0.7.3",
    "grunt-browserify": "3.2.0",
    "grunt-composer": "^0.4.4",
    "grunt-contrib-compress": "~0.10.0",
    "grunt-contrib-uglify": "^0.9.2",
    "grunt-contrib-watch": "0.6.1",
    "grunt-mocha-test": "^0.12.7",
    "grunt-phpunit": "^0.3.6",
    "jquery": "^2.1.3",
    "jsdom": "^3.1.2",
    "load-grunt-tasks": "~0.5.0",
    "mocha": "^2.2.1",
    "mockery": "^1.4.0",
    "sinon": "^1.14.1",
    "sinon-chai": "^2.7.0",
    "underscore": "^1.8.3"
  },
  "dependencies": {
    "debug": "~1.0.2",
    "fvd": "^1.0.0"
  }
}
